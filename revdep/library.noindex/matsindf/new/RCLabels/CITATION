citHeader("To cite RCLabels in publications use:")

citEntry(
  entry       = "Manual",
  title       = "RCLabels: Tools for munging and manipulating row and column labels on matrices",
  author      = personList(as.person("Matthew Kuperus Heun")),
  year        = 2022,
  note        = "R package version 0.1.1",
  url         = "https://github.com/MatthewHeun/RCLabels",
  textVersion =  paste("Matthew Kuperus Heun (2022).",
                       "RCLabels: Tools for munging and manipulating row and column labels on matrices.",
                       "R package version 0.1.1.")
)
